{
  "name": "Get [F]it Done",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Blocks pages based on fitbit data to encourage user to move around",
  "key": "",
  "browser_action":{
    "default_title": "Get Fit Done",
    "default_popup" : "popup/src/index.html"
  },
  // "content_scripts": [
  //   {
  //     "matches": ["http://*/*", "https://*/*"],
  //     "css": ["content/src/scripts/style.css"],
  //     "js": ["content/src/bundle.js"]
  //   }
  // ],
  "options_ui": {
    "page": "options/src/index.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "background": {
    "scripts": [
      "background/bundle.js"
    ]
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "storage",
    "https://api.fitbit.com/oauth2/token",
    "identity"
  ],
  "web_accessible_resources": [
      "background/index.html",
      "content/src/scripts/style.css",
      "content/src/bundle.js"
  ]
}
