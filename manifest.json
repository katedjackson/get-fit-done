{
  "name": "Get [F]it Done",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Blocks pages based on fitbit data to encourage user to move around",
  "key": "",
  "browser_action":{
    "default_title": "Get Fit Done",
    "default_popup" : "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content/style.css"],
      "js": ["build/content.js"],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options/index.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "background": {
    "scripts": [
      "build/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequestBlocking",
    "storage",
    "https://api.fitbit.com/oauth2/token",
    "identity"
  ],
  "web_accessible_resources": [
      "content/style.css"
  ]
}
